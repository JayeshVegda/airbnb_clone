    <%- include('../includes/navbar') %>
    
    
   
    
    <!-- Main Content -->
    <main class="container py-5">
        <%- include('../includes/flash') %>
        <%- body %>
    </main>
    
    <!-- Footer -->
    <%- include('../includes/footer') %>
    
    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    
    <!-- Custom JS -->
